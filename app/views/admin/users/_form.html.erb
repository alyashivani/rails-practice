<%= form_with model: @edituser,url: admin_user_path do |form| %>
  <div>
    <%= form.label :name, "Name" %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :gender, "Gender" %><br>
    <%= form.radio_button :gender, "Male" %>
    <%= form.label :gender, "Male" %><br>
    <%= form.radio_button :gender, "Female" %>
    <%= form.label :gender, "Female" %><br>
  </div>

  <div>
    <%= form.label :email, "Email" %><br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :phoneNumber, "Phone Number" %><br>
    <%= form.telephone_field :phoneNumber %>
  </div>

  <div>
    <%= form.label :dateBirth, "Date of Birth" %><br>
    <%= f.date_select  :dateBirth, start_year: Date.current.year, end_year: 1950, autofocus: true %>
  </div>

  <div>
    <%= form.label :admin, "Admin" %><br>
    <%= form.check_box :admin %>
  </div>

  

  <div>
    <%= form.submit %>
  </div>
  <div>
    <%= link_to "Back", :back %>
  </div>

<% end %>